/* pages/report/report.wxss */
page{
  overflow-y: auto !important;
}
.content{
  height: 100%;
}
.report-canvas{
  position: fixed;
  top: -10000px;
}
/* 返回按钮 */
.back-btn{
  position: fixed;
  z-index: 6;
  width: 32px;
  height: 32px;
  line-height: 32px;
  /* top:32px; */
  left: 14px;
}
.back-btn image{
  width: 8px;
  margin: auto;
  margin-top: 8px;
}
/* 底部按钮 */
.btn-box{
  z-index: 9;
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 176rpx;
  background-image: linear-gradient(-180deg, rgba(14,26,99,0.67) 1%, #0E1A63 23%);
  font-size: 0;
}
.save-btn{
  width: 325rpx;
  height: 100rpx;
  margin-left: 40rpx;
  margin-top: 36rpx;
  float: left;
  background: url(data:image/png;base64,) 0 0 no-repeat;
  background-size: 325rpx 100rpx;
}
.share-btn{
  width: 325rpx;
  height: 100rpx;
  margin-left: 20rpx;
  margin-top: 36rpx;
  float: left;
  background-image: linear-gradient(56deg, #FFD422 0%, #FD6C65 32%, #FD397D 77%, #FC2393 100%);
  box-shadow: 0 2px 14px 0 rgba(253,59,124,0.40);
  border-radius: 100px;
  font-size: 32rpx;
  color: #181063;
  text-align: center;
  line-height: 100rpx;
}
/* toast */
.toast{
  position: fixed;
  opacity: 0.9;
  background: #0E1131;
  border-radius: 8px;
  width: 380rpx;
  padding: 40rpx;
  color: #fff;
  text-align: center;
  left: 50%;
  top:45%;
  transform: translate(-50%,-50%);
  z-index: 10;
  font-size: 12px;
  line-height: 16px;
  box-sizing: border-box;
}
.toast .icon{
  width: 52rpx;
  height: 52rpx;
  margin: 0 auto 10px;
  display: flex;
}