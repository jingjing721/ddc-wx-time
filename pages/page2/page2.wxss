/* pages/page2/page2.wxss */
image{
  display: flex;
} 
.bg{
  background: url(data:image/jpeg;base64,);
  background-size: 100% 100%;
  width: 100%;
  height: 100%;
} 
/*  */
.back-btn{
  position: fixed;
  z-index: 6;
  width: 32px;
  height: 32px;
  line-height: 32px;
  /* top:32px; */
  left: 14px;
}
.back-btn image{
  width: 8px;
  margin: auto;
  margin-top: 8px;
}
/*  */
.title-img{
  width: 750rpx;
  height: 500rpx;
  position: relative; 
  margin: 0;
  padding: 0;
  margin-bottom: 40rpx;
}
.tab{
  width: 734rpx;
  height: 100rpx;
  font-size: 0;
  white-space: nowrap;
  padding:0 4rpx !important;
}
.normal-tab{
  position: absolute;
  bottom: 4rpx;
  left: 10rpx;
  right: 10rpx;
  overflow: hidden;
  margin:0 auto;
  height:100rpx;
}
.tab-item{
  display: inline-block;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 14px;
  text-align: center;
  color: #CAE0FF;
  /* width: 56px; */
  margin:0 20rpx;
  position: relative;
}
/* .tab-fix{
  position: absolute;
  background:#0E1A63;
  top:138rpx;
  left: 0;
  right: 0;
  z-index: 3;
} */
.show{
  display: block;
  animation: tab-position .2s ease-in 1;
  animation-fill-mode: forwards;
  z-index: 3;
  position: fixed;
  background:#0E1A63;
  top:0;
  left: 0;
  right: 0;
}
.hidden{
  display: none;
  /* animation: tab-position-hidden .2s ease-in 1;
  animation-fill-mode: forwards; */
}
@keyframes tab-position{
    0%{opacity: .3;}
    100%{ opacity: 1;}
}
@keyframes tab-position-hidden{
    0%{opacity: 1;z-index: 3}
    100%{ opacity: 0;z-index: -1;height: 0;}
}
.tab-item.act{
  color: #FC477E;
}
.tab-item.act::after{
  content: '';
  display: block;
  position: absolute;
  width: 38rpx;
  height: 6rpx;
  left: 50%;
  margin-left: -19rpx;
  bottom: 20rpx;
  background: linear-gradient(45deg, #FF6583 0%, #FF794D  50%, #FFBE67 100%);
}

/*  */
.list-box{
  /* margin: 40rpx 0; */
  margin-bottom: 40rpx;
}
.dish-bg{
  width: 750rpx;
  height: 88rpx;
  position: relative;
  margin-bottom: 10rpx;
  margin-top: 0;
  padding: 0;
}
.dish-box{
  box-sizing: border-box;
  padding: 0 28rpx;
}
.dish-title{
  position: absolute;
  top:0;
  left: 0;
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
  color: #fff;
  text-align: center;
}
.dish-item{
  height: 72rpx;
  line-height: 72rpx;
  box-sizing: border-box;
  margin-right: 20rpx;
  width: 218rpx;
  text-align: center;
  display: inline-block;
  color: #76B0FE;
  font-size: 24rpx;
  margin-bottom: 20rpx;
  border-radius: 40px;
  background: url(data:image/png;base64,) 0 0 no-repeat;
  background-size: 218rpx 72rpx;
}
.dish-item:nth-child(3n){
  margin-right: 0;
}
.dish-item.select{
  background-image: linear-gradient(-128deg, #FF687F 0%, #FF9E6A 100%);
  color: #181063;
}

.custom-text{
  margin: 0 28rpx;
  height: 40px;
  line-height: 40rpx;
  background: rgba(255, 255, 255, 0.15);
  border-radius: 100px;
  padding:0 20px;
  font-size: 14px;
  color: #fff;
}

/*  */
.btn-bg{
  position: fixed;
  z-index: 3;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 90px;
  background-image: linear-gradient(-180deg, rgba(31,11,91,0.00) 1%, #1D0B5B 49%);
}
.btn{
  width: 456rpx;
  height: 50px;
  margin: 20px auto;
  text-align: center;
  line-height: 50px;
  font-size: 16px;
  border-radius: 100px;
}
.btn-normal{
  background-image: linear-gradient(56deg, #FFD422 0%, #FD6C65 32%, #FD397D 77%, #FC2393 100%);
  box-shadow: 0 2px 14px 0 rgba(253,59,124,0.40);
  color: #181063;
}
.btn-unable{
  background-image: linear-gradient(45deg, #7F8698 0%, #384056 100%);
  box-shadow: 0 2px 14px 0 rgba(14,26,99,0.60);
  color: #fff;
}

/* toast */
.toast{
  position: fixed;
  opacity: 0.9;
  background: #0E1131;
  border-radius: 8px;
  width: 380rpx;
  padding: 40rpx;
  color: #fff;
  text-align: center;
  left: 50%;
  top:45%;
  transform: translate(-50%,-50%);
  z-index: 10;
  font-size: 12px;
  line-height: 16px;
  box-sizing: border-box;
}
.toast .icon{
  width: 52rpx;
  height: 52rpx;
  margin: 0 auto 10px;
  display: flex;
}